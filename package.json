{
  "name": "alphabot",
  "description": "A discord bot",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src/api",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Ilja Busch",
    "email": "ilja.ragnar@gmail.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src/api",
    "test": "test/",
    "config": "config/",
    "src": "./src"
  },
  "engines": {
    "node": "^15.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run jest",
    "lint": "eslint ./ test/. --config .eslintrc --ext .ts --fix --color",
    "dev": "npm-run-all --parallel bot api --print-label",
    "bot": "nodemon --exec \"npm run lint && npm run bot-nowatch\"",
    "bot-nowatch": "ts-node ./src/index.ts && npm run lint",
    "api": "ts-node-dev --no-notify src/api/",
    "start": "npm run compile && node lib/",
    "setup": "autosetup ./steps.setup",
    "jest": "jest --forceExit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.11",
    "@feathersjs/authentication-local": "^4.5.11",
    "@feathersjs/authentication-oauth": "^4.5.11",
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/socketio": "^4.5.11",
    "@feathersjs/transport-commons": "^4.5.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-sequelize": "^6.2.0",
    "firebase-admin": "^9.8.0",
    "helmet": "^4.6.0",
    "sequelize": "^6.6.2",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.0.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@justmedev/autosetup": "^1.0.0",
    "@types/bluebird": "^3.5.35",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/serve-favicon": "^2.5.2",
    "@types/validator": "^10.11.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "axios": "^0.21.1",
    "eslint": "^7.27.0",
    "eslint-plugin-extra-rules": "^0.0.0-development",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
